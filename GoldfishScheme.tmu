    (check (max 3 2 1) =\<gtr\> 3)

    (check (max -1 -2 -3) =\<gtr\> -1)

    (check (max 0 0 0) =\<gtr\> 0)

    (check (max 1.5 2.5 3.5) =\<gtr\> 3.5)

    (check (max 1/2 3/4 1/4) =\<gtr\> 3/4)

