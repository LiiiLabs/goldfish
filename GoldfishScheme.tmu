    (check (string-trim-right " \ 2 4 \ " 3) =\<gtr\> " 4")

    (check (string-trim-right " \ 2 4 \ " 4) =\<gtr\> "4")

    (check (string-trim-right " \ 2 4 \ " 5) =\<gtr\> "")

