  <paragraph|vector-fold><index|vector-fold>

  <\scm-chunk|goldfish/srfi/srfi-133.scm|true|true>
    (define (vector-fold f initial vec)

    \ \ (let loop ((i 0) (acc initial))

    \ \ \ \ (if (\<less\> i (vector-length vec))

    \ \ \ \ \ \ \ \ (loop (+ i 1) (f (vector-ref vec i) acc))

    \ \ \ \ \ \ \ \ acc)))

    \;
  </scm-chunk>

  <\scm-chunk|tests/goldfish/liii/vector-test.scm|true|true>
    (check (vector-fold + 0 #(1 2 3 4)) =\<gtr\> 10) \ ; 1 + 2 + 3 + 4 = 10

    (check (vector-fold * 1 #(1 2 3 4)) =\<gtr\> 24) \ ; 1 * 2 * 3 * 4 = 24

    \;
  </scm-chunk>

