    (check (vector-filter (lambda (x) (\<gtr\> x 3)) #(1 2 3 4 5 6)) =\<gtr\> #(4 5 6))

    (check (vector-filter (lambda (x) (string? x)) #(1 "a" 2 "b" 3)) =\<gtr\> #("a" "b"))

    (check (vector-filter (lambda (x) #t) #()) =\<gtr\> #())

    (check (vector-filter (lambda (x) #f) #(1 2 3)) =\<gtr\> #())

    \;
  </goldfish-chunk>

