  <\scm-chunk|goldfish/scheme/base.scm|true|true>
    ; 0-clause BSD by Bill Schottstaedt from S7 source repo: s7test.scm

    (define-macro (define-values vars expression)

    \ \ `(if (not (null? ',vars))

    \ \ \ \ \ \ \ (varlet (curlet) ((lambda ,vars (curlet)) ,expression))))

    \;
  </scm-chunk>

