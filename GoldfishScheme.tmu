    \ \ (string-append name " is " (number-\<gtr\>string age) " years old"))

    \;

    (check (person :age 21) =\<gtr\> "Bob is 21 years old")

    (check (person :name "Alice" :age 25) =\<gtr\> "Alice is 25 years old")

