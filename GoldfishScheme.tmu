    \ \ (string-for-each-index

    \ \ \ \ (lambda (i c acc)

    \ \ \ \ \ \ (cons (list i c) acc))

    \ \ \ \ "hello")

    \ \ =\<gtr\> '((0 #\\h) (1 #\\e) (2 #\\l) (3 #\\l) (4 #\\o)))

    \;
