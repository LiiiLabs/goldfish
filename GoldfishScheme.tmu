    (check (bytevector) =\<gtr\> #u8())

    (check (bytevector 1 2 3) =\<gtr\> #u8(1 2 3))

    \;

    (check (bytevector 255) =\<gtr\> #u8(255))

    (check-catch 'wrong-type-arg (bytevector 256))

    (check-catch 'wrong-type-arg (bytevector -1))

    \;
  </scm-chunk>

