  <paragraph|json-set*>

  <\goldfish-chunk|json/liii/json.scm|true|true>
    (define (json-set* json k0 k1_or_v . ks_and_v)

    \ \ (if (null? ks_and_v)

    \ \ \ \ \ \ (json-set json k0 k1_or_v)

    \ \ \ \ \ \ (json-set json k0

    \ \ \ \ \ \ \ \ (lambda (x)

    \ \ \ \ \ \ \ \ \ \ (apply json-set* (cons x (cons k1_or_v ks_and_v)))))))

    \;
  </goldfish-chunk>

