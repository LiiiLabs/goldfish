  <\scm-chunk|json/liii/json.scm|true|true>
    (define (json-ref* j . keys)

    \ \ (let loop ((expr j) (keys keys))

    \ \ \ \ (if (null? keys)

    \ \ \ \ \ \ \ \ expr

    \ \ \ \ \ \ \ \ (loop (json-ref expr (car keys)) (cdr keys)))))

    \;
  </scm-chunk>

