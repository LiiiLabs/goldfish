  </goldfish-chunk>

  <paragraph|rich-list%reduce-option>

  <\goldfish-chunk|goldfish/liii/lang.scm|true|true>
    (define (%reduce-option f)

    \ \ (if (null? data)

    \ \ \ \ \ \ (none)

    \ \ \ \ \ \ (option (reduce f '() data))))

