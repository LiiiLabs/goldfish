    \ \ \ \ (check (ht-fail :forall (lambda (k v)\ 

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (and (symbol? k) (number? v) (positive? v)))) =\<gtr\> #f)

