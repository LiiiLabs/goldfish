    \ \ (check (vec :sort-with (lambda (x y) (\<less\> (car x) (car y))))

    \ \ \ \ \ \ \ \ \ =\<gtr\> (rich-vector #((1 . 3) (1 . 2) (2 . 1) (3 . 3) (3 . 2))))

    \ \ (check (vec :sort-with (lambda (x y) (\<less\> (cdr x) (cdr y))))

    \ \ \ \ \ \ \ \ \ =\<gtr\> (rich-vector #((2 . 1) (1 . 2) (3 . 2) (3 . 3) (1 . 3)))))

