  <paragraph|rich-vector%slice>

  <\scm-chunk|goldfish/liii/lang.scm|true|true>
    (chained-define (%slice from until)

    \ \ (let* ((len (vector-length data))

    \ \ \ \ \ \ \ \ \ (start (max 0 from))

    \ \ \ \ \ \ \ \ \ (end (min len until)))

    \ \ \ \ (if (\<less\> start end)

    \ \ \ \ \ \ \ \ (rich-vector (vector-copy data start end))

    \ \ \ \ \ \ \ \ (rich-vector :empty))))

