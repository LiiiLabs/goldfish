    (let ((vec (rich-vector #(3 1 4 2 5))))

    \ \ (check (vec :sort-with \<less\>) =\<gtr\> (array #(1 2 3 4 5)))

    \ \ (check (vec :sort-with \<gtr\>) =\<gtr\> (array #(5 4 3 2 1)))

    \ \ (check (vec :sort-with \<less\> :collect) =\<gtr\> #(1 2 3 4 5)))

