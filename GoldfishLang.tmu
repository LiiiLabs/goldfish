    (check ($ 8 :sqrt) =\<gtr\> 2)

    (check ($ 10 :sqrt) =\<gtr\> 3)

    (check ($ 144 :sqrt) =\<gtr\> 12) \ \ \ \ \ \ 

    (check ($ 289 :sqrt) =\<gtr\> 17) \ \ \ \ \ \ 

    (check ($ 290 :sqrt) =\<gtr\> 17) \ \ \ \ \ \ 

    (check ($ 10201 :sqrt) =\<gtr\> 101) \ \ \ \ \ \ 

    (check ($ 10403 :sqrt) =\<gtr\> 101)\ 

    (check ($ (rich-integer :max-value) :sqrt) =\<gtr\> 3037000499)

    (check-catch 'value-error ($ -1 :sqrt))

    \;
  </goldfish-chunk>

